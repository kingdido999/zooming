{
  "name": "zooming",
  "version": "2.1.1",
  "description": "Image zoom that makes sense.",
  "main": "build/zooming.js",
  "module": "build/zooming.module.js",
  "jsnext:main": "build/zooming.module.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:kingdido999/zooming.git"
  },
